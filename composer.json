{
	"name": "valadanchik/migrations-generator",
	"description": "Generates Laravel Migrations from an existing database || Created as an example Bernhard Breytenbach Xethron package",
	"keywords": ["laravel", "migration", "generator", "migrations", "artisan"],
	"license": "MIT",
	"authors": [
		{
			"name": "Volodya Sargsyan",
			"email": "sar.volodya@gmail.com"
		}
	],
	"require": {
		"php": "^7.2",
		"illuminate/support": "^5.5 || ^6.0",
		
		"doctrine/dbal": "~2.4"
	},
	"autoload": {
		"psr-0": {
			"Valadanchik\\MigrationsGenerator": "src/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": ">=4.0.0",
		"mockery/mockery": ">=0.9.0",
		"illuminate/cache": ">=4.1.0",
		"illuminate/console": ">=4.1.0"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Way\\Generators\\GeneratorsServiceProvider",
				"Valadanchik\\MigrationsGenerator\\MigrationsGeneratorServiceProvider"
			]
		}
	}
}
